<configuration>
	<configSections>
		<section name="rebus" type="Rebus.XmlConfig.RebusConfigurationSection, Rebus.XmlConfig" />
	</configSections>
	<rebus>
		<endpoints>
			<add messages="Rebus.Sample.Contracts" endpoint="Rebus.Server:MailBox@Rebus:Server" />
			<add messages="Rebus.LegacySample.Contracts" endpoint="Rebus.LegacySample:Mailbox" />
		</endpoints>
	</rebus>
</configuration>